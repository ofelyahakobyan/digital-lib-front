{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "experimentalObjectRestSpread": true
  },
  "rules": {
    "react/jsx-props-no-spreading": "off",
    "no-shadow": "off",
    "react/jsx-filename-extension": [
      "error",
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "jsx-quotes": [
      "error",
      "prefer-double"
    ]
    //rules must be added here
  }
}
